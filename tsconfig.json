{
	"compileOnSave": true,
	"angularCompilerOptions": {
		"strictTemplates": true,
	},
	"compilerOptions": {
		"skipLibCheck": true,
		"baseUrl": "./",
		"outDir": "./dist/out-tsc",
		"sourceMap": true,
		"declaration": false,
		"module": "esnext",
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"target": "es5",
		"paths": {
			"@core/services/*": [
				"src/app/core/_base/layout/service/*"
			]
		},
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2018",
			"dom"
		],
		"allowSyntheticDefaultImports": true
	}
}